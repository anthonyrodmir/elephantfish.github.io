<body>

<div class="slider-container">
    <iframe id="imageFrame" src="assets/img/2023-11-10-A-Transformer-Based-Approach-for-Simulating-Ecological-Recovery/band/band_1.html"></iframe>
    <input type="range" min="1" max="200" value="1" class="slider" id="imageSlider" oninput="updateImage(this.value)">
</div>

<script>
    function updateImage(imageNumber) {
        var iframe = document.getElementById('imageFrame');
        iframe.src = 'assets/img/2023-11-10-A-Transformer-Based-Approach-for-Simulating-Ecological-Recovery/band' + imageNumber + '.html';
    }
</script>

